{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      "never",
      {
        "js": "always",
        "json": "always"
      }
    ]
  }
}
